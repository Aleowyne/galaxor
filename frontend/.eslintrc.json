{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": "off",
    "brace-style": "off",
    "import/extensions": 0,
    "no-shadow": "off",
    "max-len": [
      "error",
      {
        "code": 150
      }
    ]
  }
}